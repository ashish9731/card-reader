{
  "version": 2,
  "builds": [
    {
      "src": "card_reader3.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "includeFiles": [
          "**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "card_reader3.py"
    }
  ],
  "functions": {
    "card_reader3.py": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}